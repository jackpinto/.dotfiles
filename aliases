alias e='vim'
alias www='cd /www'
alias gen='rails g'
alias gg='git pull origin master'
alias ll='ls -l'
alias l='ll'
alias pp='git push'
alias aa='annotate --before'
alias cc='cap deploy:cold'
alias dd='cap deploy'
alias vim='mvim -N'
alias downloads='cd ~/Downloads'
alias gem='sudo gem'
alias gitabort='git rebase -i HEAD~5'
alias process='ps -ax | grep'
alias port='sudo port'
alias vim-bundle='cd ~/.vim/bundle/; git clone'
alias yearofmoo='cd /www/yearofmoo.com'
alias dc='cd -'
alias ..='cd ..'
alias ea='vim ~/.bash/aliases'

alias mysqlstart='sudo /opt/local/bin/mysqld_safe5 &amp;'
alias mysqlstop='/opt/local/bin/mysqladmin5 -u root -p shutdown'

export PATH="/usr/local/bin:/usr/local/sbin:/usr/local/mysql/bin:$PATH" 

#prohpc
alias prohpc='cd /www/pro-hpc.com'
alias prohpc.watch='
cd /www/pro-hpc.com/public/stylesheets/public/
sass --watch common.scss:common.css &
sass --watch forms.scss:forms.css &
sass --watch pages.scss:pages.css &
sass --watch layout.scss:layout.css &'
alias prohpc.edit='mate /www/pro-hpc.com'
alias prohpc.server='cd /www/pro-hpc.com; /www/pro-hpc.com/script/rails s &'
alias prohpc.ready='
prohpc.watch
prohpc.server'
alias prohpc.develop='
prohpc.edit
prohpc.ready
prohpc'

alias wirehead='cd /www/wirehead.ca'
alias wirehead.server='cd /www/wirehead.ca; /www/wirehead.ca/script/rails s &'
alias wirehead.edit='mate /www/wirehead.ca'
alias wirehead.develop='
wirehead.edit
wirehead.server'
